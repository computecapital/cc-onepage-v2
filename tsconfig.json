{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "types": [],
    "typeRoots": [
      "./src/types",
      "./node_modules/@types"
    ],
    "baseUrl": ".",
    "paths": {
      "figma:asset/*": [
        "src/assets/*"
      ]
    }
  },
  "include": [
    "src",
    "vite.config.ts"
  ]
}


